# General settings
screenshot-extension png
show-frame-rate-meter #t
frame-rate-meter-update-interval 0.01
icon-filename phase_3/etc/icon.ico
cursor-filename phase_3/etc/toonmono.cur

# Graphical settings
default-antialias-enable 1
framebuffer-multisample 1
interpolate-frames true

# Window settings:
window-title Toontown Toon Creator
win-origin -2 -2
win-size 1920 1080
fullscreen false

# Model config
default-model-extension .bam
model-cache-models #t
model-cache-textures #t